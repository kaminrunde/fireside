{
  "storybookUrl": "http://localhost:6006/",
  "defaultStory": "cms/base/Banner/Builder",
  "mediaSizes": [
    {
      "label": "mobile",
      "key": "MOBILE",
      "icon": "MOBILE",
      "gap": 20,
      "breakpoint": 0,
      "initialyActive": true
    },{
      "label": "tablet",
      "key": "TABLET",
      "icon": "TABLET",
      "gap": 20,
      "breakpoint": 500,
      "initialyActive": false
    },{
      "label": "laptop",
      "key": "LAPTOP",
      "icon": "LAPTOP",
      "gap": 20,
      "breakpoint": 800,
      "initialyActive": false
    }
  ],
  "connector": "@kaminrunde/fireside-connector-contentful",
  "plugins": [
    {
      "resolve": "@kaminrunde/fireside-plugin-fullwidth-components",
      "options": {
        "key": "fullWidth"
      }
    },{
      "resolve": "fireside-plugin-component-gap",
      "options": {
        "key": "componentGap"
      }
    }
  ]
}