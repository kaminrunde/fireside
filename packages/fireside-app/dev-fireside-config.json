{
  "storybookUrl": "http://localhost:6006/",
  "defaultStory": "cms/base/Banner",
  "mediaSizes": [
    {
      "label": "mobile",
      "key": "MOBILE",
      "icon": "MOBILE",
      "gap": 20,
      "breakpoint": 0,
      "initialyActive": true
    },{
      "label": "tablet",
      "key": "TABLET",
      "icon": "TABLET",
      "gap": 20,
      "breakpoint": 500,
      "initialyActive": false
    },{
      "label": "laptop",
      "key": "LAPTOP",
      "icon": "LAPTOP",
      "gap": 20,
      "breakpoint": 800,
      "initialyActive": false
    }
  ],
  "connector": "modules/connector/utils/demoConnector",
  "plugins": [
    {
      "resolve": "@kaminrunde/fireside-plugin-fullwidth-components",
      "options": {
        "key": "fullWidth"
      }
    },
    {
      "resolve": "@kaminrunde/fireside-plugin-background-rows",
      "options": {
        "key": "bg"
      }
    }
  ]
}